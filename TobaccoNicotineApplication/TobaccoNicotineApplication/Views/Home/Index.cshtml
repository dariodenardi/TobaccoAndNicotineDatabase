@{
    ViewBag.Title = "Home Page";
    ViewBag.PageTitle = "Dashboard";
    ViewBag.TitlePrincipal = "General Information";
}

@if (TempData["Message"] != null)
{
    <div class="alert alert-success">
        @TempData["Message"]
    </div>
}

@section styles {
    <!-- Sweet Alert -->
    <link href="~/Scripts/plugins/bootstrap-sweetalert/sweet-alert.css" rel="stylesheet" type="text/css">

    <!-- jvectormap -->
    <link href="~/Scripts/plugins/jvectormap/jquery-jvectormap-2.0.2.css" rel="stylesheet">
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary">
            <div class="panel-body">
                <h4 class="m-t-0 m-b-30">Tobacco&Nicotine World Map</h4>
                <div id="world-map-markers" style="height: 400px"></div>
            </div>
        </div>
    </div>
</div>

<div class="row" style="visibility:hidden;" id="blockValuesNotNull">
    <div class="col-md-4">
        <div class="panel">
            <div class="panel-body">
                <h4 class="m-b-30 m-t-0" id="textValuesNotNull"></h4>

                <div id="insertValuesNotNull">
                </div>

            </div>
        </div>
    </div> <!-- end col -->
</div>
<!-- end row -->

@section scripts {
    <!-- Sweet-Alert  -->
    <script src="~/Scripts/plugins/bootstrap-sweetalert/sweet-alert.min.js" type="text/javascript"></script>

    <script src="~/Scripts/plugins/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="~/Scripts/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Scripts/plugins/jvectormap/gdp-data.js"></script>
    <script src="~/Scripts/plugins/jvectormap/jvectormap.init.js"></script>
}